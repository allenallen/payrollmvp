<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tamaraw.payroll.controllers.DeductionTypeController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="326.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onExitMenuItemClicked" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onAddMenuItemClicked" text="Add" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="tableViewDeductionTypes" layoutY="27.0" prefHeight="373.0" prefWidth="326.0">
        <columns>
          <TableColumn fx:id="tableColumnType" prefWidth="200.0" text="Type" />
          <TableColumn fx:id="tableColumnDelete" prefWidth="125.0" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
