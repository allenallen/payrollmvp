<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="195.0" prefWidth="284.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tamaraw.payroll.controllers.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="label" text="Label" visible="false" />
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Label prefHeight="17.0" prefWidth="104.0" text="Username">
            <padding>
               <Insets top="5.0" />
            </padding>
         </Label>
         <TextField id="userName" fx:id="username" />
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Label prefHeight="17.0" prefWidth="104.0" text="Password">
            <padding>
               <Insets top="5.0" />
            </padding>
         </Label>
         <PasswordField fx:id="password" />
      </children>
   </HBox>
    <Button onAction="#onLoginBtnClicked" text="Login" />
</VBox>
